    <style name="TimerButtonRed01Style">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/ondark_01</item>
        <item name="android:lineSpacingExtra">0sp</item>
        <item name="android:background">@drawable/shape_timer_background_red</item>
    </style>

    <style name="DialogButtonRed01Style">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/error_200</item>
        <item name="android:lineSpacingExtra">0sp</item>
        <item name="android:padding">4dp</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="DialogButtonPrimary200Style">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/primary_200</item>
        <item name="android:padding">4dp</item>
        <item name="android:lineSpacingExtra">0sp</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="SheetListDialog" parent="Theme.AppCompat.Dialog">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.3</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="DarkDialog" parent="Theme.AppCompat.Dialog.Alert">
        <item name="android:titleTextColor">@color/ondark_01</item>
        <item name="android:buttonBarPositiveButtonStyle">@style/DialogButtonRed01Style</item>
        <item name="android:buttonBarNegativeButtonStyle">@style/DialogButtonPrimary200Style</item>
        <item name="android:windowBackground">@color/background_500</item>
    </style>
</resources>